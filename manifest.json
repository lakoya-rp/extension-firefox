{

    "manifest_version": 2,
    "name": "Lakoya RP",
    "version": "0.5",
  
    "description": "Cette extension permet d'obtenir le nombre de joueurs sur LakoyaRP depuis ton navigateur",
    "homepage_url": "https://opensource.lakoya-rp.fr",
  
    "icons": {
      "48": "img/logo_400x400.png"
    },
  
    "browser_action": {
        "default_icon": "img/logo_400x400.png",
        "default_title": "LakoyaRP",
        "default_popup": "script/popup.html"
      }
    }